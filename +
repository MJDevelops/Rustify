use std::{error::Error, process::exit};
use rustify::auth::get_client_credentials;
use tokio::sync::MutexGuard;

#[tokio::main]
async fn main() -> Result<(), Box<dyn Error>> {
    let creds = get_client_credentials().unwrap_or_else(|err| {
        eprintln!("{}", err);
        exit(1);
    });

    let token: MutexGuard<_> = creds.token.lock().await.unwrap();



    println!("{:?}", ) ;
    Ok(())
}
